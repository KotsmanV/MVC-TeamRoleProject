@model IEnumerable<TeamRoleProject.Models.Book>

@{
    ViewBag.Title = "Index";
}

<h2>Catalog</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>Actions</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.BookId })
        </td>
    </tr>
}

</table>
